.red_bg {
	background:#ff2d4b;
}
.red_txt {
	color:#ff2d4b;
}
.blue_bg {
	background:#40bff5;
}
.green_bg {
	background:#9acf4f;
}

body {
	background:#f8f8f8;
}
.disBlock {
	display:block;
}
.has-solded {
	margin-right:0.4rem;
}
.slider-main {
	height:16rem;
	overflow:hidden;
}
.swiper-wrapper img {
	width:100%;
	height:100%;
}
.swiper-slide {
	background:url(../images/big-bg.png) no-repeat;
	background-size:cover;
}
.swiper-slider {
	height:16rem;
	max-width:16rem;
}
.swiper-span {
	position:absolute;
	width:2.1rem;
	height:0.9rem;
	line-height:1rem;
	background:rgba(0,0,0,0.4);
	border-radius:40px;
	color:#fff;
	font-size:0.65rem;
	text-align:center;
	right:0.75rem;
	bottom:0.75rem;
	z-index:222;
}
.detail-header,
.img-wrap,
.info-table,
.comment-main  {
	background:#fff;
	margin-bottom:0.5rem;
}
.detail-header-title {
	margin:0 0.75rem;
	padding-top:0.75rem;
}
.fl-title {
	display:-webkit-box;
	-webkit-line-clamp:2;
	text-overflow:ellipsis;
	-webkit-box-orient:vertical;
	overflow:hidden;
	font-size:0.7rem;
	width:11.5rem;
	line-height:0.9rem;
	padding-right:0.75rem;
	word-break: break-all;
	word-wrap: break-word;
	min-height:1.8rem;
}
.br-right {
	width:1px;
	height:1.7rem;
	float:left;
	display:block;
	background:#d6d6d6;
}
.fr-title {
	color:#a0a0a0;
	font-size:0.55rem;
	text-align:center;
	margin-left:0.85rem;
}
.fr-title i {
	background:url(../images/btn_like_normal.png) no-repeat;
	background-size:100% auto;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
	width:0.95rem;
	height:0.85rem;
	display:block;
	margin:0 auto 0.25rem;
}
.fr-title i.zan {
	background:url(../images/btn_like_selected.png) no-repeat;
	background-size:100% auto;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
	width:0.95rem;
	height:0.85rem;
	display:block;
	margin:0 auto 0.25rem;
	animation:scale 0.6s ease-in;
	-webkit-animation:scale 0.6s ease-in;
}
@keyframes scale {
	50% {
		transform:scale(1.2);
	}
}
@-webkit-keyframes scale {
	50% {
		-webkit-transform:scale(1.2);
	}
}
.title {
	margin-bottom:0.35rem;
}
.price-current {
	font-size:1rem;
	font-family:"verdana";
	margin-right:0.2rem;
	vertical-align:middle;
	margin-right:0.5rem;
}
.price-current em{
	margin-left:0.1rem;
}
.price-current i {
	font-size:0.65rem;
}
.price-ui-icon>p:nth-child(1) {
	margin-bottom:0.4rem;
	float:left;
}
.price-ui-icon .dis_icon span {
	padding: 0 0.3rem;
	height:0.85rem;
	border-radius:2px;
	line-height:0.9rem;
	display:inline-block;
	color:#fff;
	font-size:0.5rem;
	text-align:center;
	vertical-align:middle;
	margin-right:0.25rem;
}
.market-price {
	font-size:0.6rem;
	color:#a0a0a0;
	float:left;
}
.market-price i {
	text-decoration:line-through;
}
.sold-num {
	float:right;
	font-size:0.6rem;
}
.discount-percent {
	padding-bottom:0.5rem;
}
.count-down {
	height:2rem;
	line-height:2rem;
	width:100%;
	max-width:640px;
	text-align:center;
	font-size:0.7rem;
	border-top:1px solid #efefef;
	display:none;
}
.clock {
	background:url(../images/icon_clock.png) no-repeat;
	background-size:100% auto;
	width:0.78rem;
	height:0.8rem;
	display:inline-block;
	vertical-align:middle;
	margin-right:0.1rem;
}
.count-down i {
	color:#ff2d4b;
}
.count-down-time {
	vertical-align:middle;
	display:inline-block;
}
.img-box {
	margin:0 0.35rem;
	padding-top:0.35rem;
	padding-bottom:0.35rem;
	word-wrap: break-word;
	word-break: break-all;
	font-size:0.65rem;
	line-height:0.9rem;
}
.img-box img {
	width:100%;
}
.info-header {
	height:2rem;
	line-height:2.1rem;
	font-size:0.7rem;
	padding-left:0.75rem;
	border-bottom:1px solid #efefef;
}
.line {
	width:6px;
	height:0.68rem;
	display:inline-block;
	vertical-align:middle;
	border-radius:2px;
	margin-right:0.4rem;
}
.disCell {
	display: table-cell;
	align-items: center;
}
.disCell span {
	vertical-align:middle;
	margin-left:0.1rem;
}
.lineImg {
	width:0.2rem;
	vertical-align:middle;
	display:inline;
}
.info-box {
	margin:0.75rem;
}
.info-msg {
	padding-bottom:0.2rem;
}
.info-msg span  {
	font-size:0.6rem;
	width:50%;
	display:-webkit-box;
	-webkit-box-orient:vertical;
	-webkit-line-clamp:1;
	text-overflow:ellipsis;
	overflow:hidden;
	float:left;
	margin-bottom:0.5rem;
}
.info-msg:after {
	content:'';
	display:block;
	width:0;
	height:0;
	clear:both;
}
.comment-user {
	margin-bottom:0.5rem;
}
.comment-user img {
	width:1.5rem;
	height:1.5rem;
	display:inline-block;
	vertical-align:middle;
	border-radius:50%;
}
.comment-user .user-name {
	font-size:0.65rem;
	display:inline-block;
	vertical-align:middle;
}
.comment-txt {
	 font-size: 0.65rem;
	 color: #353535;
	 padding-bottom: 0.7rem;
	 line-height: 0.9rem;
	 word-break: break-word;
	 word-wrap: break-word;
	 -webkit-line-clamp: 2;
	 text-overflow: ellipsis;
	 display: -webkit-box;
	 -webkit-box-orient: vertical;
	 height: 1rem;
	 overflow: hidden;
	 margin-bottom: 0.7rem;
}
.comment-list ul  {
	margin:0 0.35rem;
}
.comment-list ul li {
	border-bottom:1px dashed #d6d6d6; 
	padding-top:0.75rem;
}
.comment-wrap {
	margin:0 0.35rem;
}
.comment-more {
	height:2rem;
	line-height:2rem;
	color:#353535;
	text-align:center;
	display:block;
	font-size:0.6rem;
}
.more-icon {
	background:url(../images/btn_pingjia.png) no-repeat;
	background-size:100% auto;
	width:0.75rem;
	height:0.65rem;
	display:inline-block;
	vertical-align:middle;
}
.pro-list {
	margin:0 0.35rem;
}
.pro-list ul li {
	float:left;
	width:7.4rem;
	height:11.25rem;
	margin-bottom: 0.35rem;
	border:1px solid #d6d6d6;
	position:relative;
}
.pro-list ul li  a:hover,
.pro-list ul li  a:visited,
.pro-list ul li  a:link{
	color:#000;
}
.pro-list .img {
	margin-bottom:0.35rem;
	max-height: 7.4rem;
	height: 7.4rem;
}
.pro-list img {
	width:100%;
	height:7.5rem;
	max-height:7.5rem;
}
.pro-list ul li:nth-child(even) {
	float:right;
}
.pro-list .title {
	font-size:0.65rem;
	text-overflow:ellipsis;
	display:-webkit-box;
	-webkit-line-clamp:2;
	-webkit-box-orient:vertical;
	overflow:hidden;
}
.pro-list .price {
	border-top:1px solid #efefef;
	padding-top:0.5rem;
}
.pro-list ul {
	margin-top:0.35rem;
}
.pro-list ul:after {
	display:block;
	content:'';
	width:0;
	height:0;
	clear:both;
}
.nr {
	margin:0 0.4rem;
}
.pro-list .price .current_price {
	font-size:0.7rem;
	float:left;
	margin-right:0.2rem;
}
.pro-list .price .mark_price {
	font-size:0.5rem;
	color:#a0a0a0;
	float:left;
	text-decoration:line-through;
	margin-top: 0.2rem;
}
.stips {
	width: 1.35rem;
    height: 0.7rem;
    background: #ff2d4b;
    text-align: center;
    color: #fff;
    border-radius: 4px;
    line-height: 0.7rem;
    display: inline-block;
    font-size: 0.45rem;
    vertical-align: middle;
    float: right;
}
.cart-box {
	border-top:1px solid #d6d6d6;
}
.cart-market {
	margin:0.45rem 0.75rem;
	position:relative;
}
.cart-icon {
	background:#fafafa;
	border:1px solid #dddddd;
	height:1.605rem;
	line-height:1.605rem;
	display:block;
	float:left;
	width:3.75rem;
	border-radius:6px;
	position:relative;
}
.cart-icon  i {
	background:url(../images/btn_cart_normal.png) no-repeat;
	background-size:100% auto;
	width:1.05rem;
	height:0.95rem;
	line-height:0.95rem;
	display:block;
	margin:0.4rem auto 0;
}
.cart-icon  em {
  border-radius: 50%;
  height: 0.7rem;
  width: 0.7rem;
  line-height: 0.7rem;
  color: #fff;
  font-size: 0.5rem;
  text-align: center;
  display: block;
  position: absolute;
  right: 1rem;
  top: 0.1rem;
}
.add_cart  {
	height:1.625rem;
	line-height:1.625rem;
	width:9.8rem;
	display:block;
	border-radius:6px;
	color:#fff;
	font-size:0.75rem;
	text-align:center;
	float:right;
}
.user-liked {
	background:#fff;
}
.comment-empty {
	text-align: center;
	min-height: 5rem;
	line-height: 5rem;
	color: #a0a0a0;
	font-size: 0.65rem;
 }
 .nonBB {
	margin-bottom:0;
 }
 
 /* 2015-8-24 add by wuyanhong */
 .discoupon {
 	border-top: 1px solid #efefef;
 	line-height: 0.5rem;
 	background: #fff;
 	padding: 0.75rem;
 	color: #353535;
 	font-size: 0.65rem;
 	display:none;
 }
 .discoupon span.disicon {
 	width:1.7rem;
 	height:0.75rem;
 	line-height:0.85rem;
 	background:#ff2d4b;
 	border-radius:4px;
 	display:inline-block;
 	vertical-align:middle;
 	font-size:0.45rem;
 	text-align:center;
 	color:#fff;
 }
 .discoupon span.disiconTxt {
 	vertical-align:middle;
 }